 <div class=\"hero-mosaic\" aria-hidden=\"true\">\n        {% assign h_img = posts[0].image | img_url: '800x' | default: 'https://via.placeholder.com/420x260?text=Image' %}\n        <div class=\"tile t1\" style=\"background-image: url('{{ h_img }}');\"></div>\n        <div class=\"tile t2\" style=\"background-image: url('{{ h_img }}');\"></div>\n        <div class=\"tile t3\" style=\"background-image: url('{{ h_img }}');\"></div>\n        <div class=\"tile t4\" style=\"background-image: url('{{ h_img }}');\"></div>\n      </div>\n    </div>\n  {% endif %}\n\n  <!-- ARTICLES -->\n  <div class=\"articles-grid\">\n    {% if posts.size > 1 %}\n      {% assign a2_excerpt = posts[1].excerpt | strip_html | default: posts[1].content | strip_html %}\n      <div class=\"article-row\">\n        <div class=\"square-thumb\" style=\"background-image: url('{% if posts[1].image %}{{ posts[1].image | img_url: '600x600' }}{% else %}https://via.placeholder.com/600x600?text=Photo{% endif %}');\"></div>\n        <div>\n          <div class=\"meta\">{{ posts[1].published_at | date: \"%b %d, %Y\" }}</div>\n          <h3 class=\"row-title\">{{ posts[1].title }}</h3>\n          <p class=\"row-excerpt\">{{ a2_excerpt | truncate: 200 }}</p>\n          <a class=\"row-cta\" href=\"{{ posts[1].url }}\">Read More</a>\n        </div>\n      </div>\n    {% endif %}\n\n    {% if posts.size > 2 %}\n      {% assign a3_excerpt = posts[2].excerpt | strip_html | default: posts[2].content | strip_html %}\n      <div class=\"article-row reverse\">\n        <div>\n          <div class=\"meta\">{{ posts[2].published_at | date: \"%b %d, %Y\" }}</div>\n          <h3 class=\"row-title\">{{ posts[2].title }}</h3>\n          <p class=\"row-excerpt\">{{ a3_excerpt | truncate: 220 }}</p>\n          <a class=\"row-cta\" href=\"{{ posts[2].url }}\">Read More</a>\n        </div>\n        <div>\n          <div class=\"shelf\">\n            {% if posts[2].image %}\n              <img src=\"{{ posts[2].image | img_url: '600x' }}\" alt=\"{{ posts[2].title }}\">\n            {% else %}\n              <img src=\"https://via.placeholder.com/260x160?text=Products\" alt=\"Products\">\n            {% endif %}\n          </div>\n        </div>\n      </div>\n    {% endif %}\n  </div>\n\n</div>"
