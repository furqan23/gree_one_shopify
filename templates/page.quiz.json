label for=\"g\">Follow an advanced routine</label>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <!-- Progress Bar -->\n            <div class=\"progress-container\">\n                <div class=\"progress-bar\" id=\"progressBar\"></div>\n            </div>\n\n            <!-- Navigation Buttons -->\n            <div class=\"navigation-button\">\n                <button id=\"prevBtn\">Previous</button>\n                <button id=\"nextBtn\">Next</button>\n            </div>\n        </div>\n    </section>\n\n\n\n\n    <script>\n        const imgSection = document.getElementById('imgSection');\n        const qaSection = document.getElementById('qaSection');\n        const startBtn = document.getElementById('startBtn');\n\n        startBtn.addEventListener('click', () => {\n            imgSection.style.display = 'none';\n            qaSection.style.display = 'block';\n            console.log(\"Start button clicked. Q&A section is now visible.\");\n        });\n\n        let currentStep = 1;\n        const totalSteps = 4;\n        const progressBar = document.getElementById(\"progressBar\");\n\n        function showStep(step) {\n            document.querySelectorAll(\".qa-content\").forEach(el => el.classList.remove(\"active\"));\n            document.getElementById(`step${step}`).classList.add(\"active\");\n\n            progressBar.style.width = `${((step - 1) / (totalSteps - 1)) * 100}%`;\n            document.getElementById(\"prevBtn\").disabled = step === 1;\n            // document.getElementById(\"nextBtn\").disabled = step === totalSteps;\n            if (step === totalSteps) {\n                document.getElementById(\"nextBtn\").textContent = \"Finish\";\n                console.log(\"Reached the last step. Next button changed to Finish.\");\n\n            } else {\n                document.getElementById(\"nextBtn\").textContent = \"Next\";\n            }\n        }\n\n        document.getElementById(\"nextBtn\").addEventListener(\"click\", function () {\n            console.log(`Next/Finish button clicked. Current step: ${currentStep}`);\n\n            if (currentStep < totalSteps) {\n                currentStep++;\n                showStep(currentStep);\n                console.log(`Moved to step ${currentStep}.`);\n            } else if (currentStep === totalSteps) {\n                const answers = {\n                    name: document.getElementById(\"name\").value,\n                    skinConcern: document.querySelector(\"input[name='question1']:checked\")?.value,\n                    benefits: document.querySelector(\"input[name='question2']:checked\")?.value,\n                    skincareApproach: document.querySelector(\"input[name='question3']:checked\")?.value\n                };\n\n                console.log(\"Collected answers:\", answers);\n\n                if (answers.name && answers.skinConcern && answers.benefits && answers.skincareApproach) {\n                    console.log(\"All answers are filled. Redirecting to results page...\");\n                    const queryString = new URLSearchParams(answers).toString();\n                    console.log(\"Query string:\", queryString);\n                    window.location.href = 'https://tursian.com/pages/quiz-results?' + queryString;\n                } else {\n                    console.log(\"Some answers are missing. Showing alert.\");\n                    alert(\"Please fill out all the questions before finishing.\");\n                }\n            }\n        });\n\n        document.getElementById(\"prevBtn\").addEventListener(\"click\", function () {\n            if (currentStep > 1) {\n                currentStep--;\n                showStep(currentStep);\n                console.log(`Moved back to step ${currentStep}.`);\n            }\n        });\n\n        showStep(currentStep);\n        console.log(\"Form initialized. Current step:\", currentStep);\n\n    </script>\n\n<