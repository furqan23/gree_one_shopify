<div class="pb-18">
    <div class="large:grid large:grid-cols-12">
        <div class="max-h-[80vh] overflow-hidden large:col-span-8">
            {%- if section.settings.video_url == blank -%}
                {%- unless section.settings.image_mobile == blank and section.settings.image == blank -%}
                    <picture>
                        <source srcset="{{ section.settings.image_mobile | image_url: width: 479 }} 1x,
                                        {{ section.settings.image_mobile | image_url: width: 958 }} 2x"
                                media="(max-width: 479px)"
                        >
                        <source srcset="{{ section.settings.image_mobile | image_url: width: 767 }} 1x,
                                        {{ section.settings.image_mobile | image_url: width: 1534 }} 2x"
                                media="(max-width: 767px)">
                        <source srcset="{{ section.settings.image_mobile | image_url: width: 1023 }} 1x,
                                        {{ section.settings.image_mobile | image_url: width: 2046 }} 2x"
                                media="(max-width: 1023px)"
                        >
                        <source srcset="{{ section.settings.image | image_url: width: 853 }} 1x,
                                        {{ section.settings.image | image_url: width: 1706 }} 2x"
                                media="(max-width: 1279px)"
                        >
                        <source srcset="{{ section.settings.image | image_url: width: 960 }} 1x,
                                        {{ section.settings.image | image_url: width: 1920 }} 2x"
                                media="(max-width: 1439px)"
                        >
                        <source srcset="{{ section.settings.image | image_url: width: 1280 }} 1x,
                                        {{ section.settings.image | image_url: width: 2560 }} 2x,">
                        <img src="{{ section.settings.image | image_url: width: 1280 }}"
                            class="object-cover w-full h-full scale-in js-scale-in"
                            width="{{ section.settings.image.width }}"
                            height="{{ section.settings.image.height }}"
                            alt="{{ section.settings.image.alt }}"
                            loading="eager"
                        >
                    </picture>
                {%- endunless -%}
            {%- else -%}
                {%- unless section.settings.video_url == blank -%}
                    {%- unless section.settings.image == blank -%}
                        {%- assign videoPoster = section.settings.image | image_url: width: 1280 -%}
                    {%- endunless -%}
                    <video class="object-cover w-full h-full scale-in transition-transform duration-500 ease-in-out js-scale-in js-responsive-video"
                        width="375"
                        height="211"
                        autoplay
                        playsinline
                        muted
                        loop
                        {% if videoPoster %}
                            poster="{{ videoPoster | image_url: '1280x530' }}"
                        {% endif %}
                        {% unless section.settings.video_url == blank %}
                            data-desktop-video="{{ section.settings.video_url }}"
                        {% endunless %}
                        {% unless section.settings.mobile_video_url == blank %}
                            data-mobile-video="{{ section.settings.mobile_video_url }}"
                        {% endunless %}
                    >
                {%- endunless -%}
            {%- endif -%}
        </div>
        <div class="pt-8 pb-[3.25rem] large:py-16 large:col-span-4 bg-{{ section.settings.background_colour }}">
            <div class="h-full flex flex-col items-start justify-center mx-auto wrap large:max-w-80 xLarge:ml-16">
                {%- unless section.settings.heading == blank -%}
                    <h1 class="heading-r text-{{ section.settings.text_colour }}">
                        {{ section.settings.heading }}
                    </h1>
                {%- endunless -%}
                {%- unless section.settings.text == blank -%}
                    <div class="wysiwyg text-{{ section.settings.text_colour }} mt-4 medium:text-body-l large:mt-8">
                        {{- section.settings.text -}}
                    </div>
                {%- endunless -%}
                {%- unless section.settings.button_link == blank -%}
                    <div class="mt-6 large:mt-3.5">
                        <a class="button button--hero{% if section.settings.button_style %} button--{{ section.settings.button_style }}{% endif %}" href="{{ section.settings.button_link }}">
                            <div class="button__label">
                                <span>
                                    {{ section.settings.button_text }}
                                </span>
                                <span class="absolute top-10 left-0">
                                    {% if section.settings.button_text %}
                                        {{ section.settings.button_text }}
                                    {% else %}
                                        {{ section.settings.secondary_button_text }}
                                    {% endif %}
                                </span>
                            </div>
                        </a>
                    </div>
                {%- endunless -%}
                {% comment %} Needs replacing with actual trustpilot widget {% endcomment %}
                {% if section.settings.trustpilot %}
                    <div class="mt-12 large:mt-[3.75rem]">
