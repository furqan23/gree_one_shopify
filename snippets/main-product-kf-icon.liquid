<li class="flex items-center gap-x-3 xLarge:gap-x-2">
    {%- if icon != 'none' and icon != blank -%}
        <div class="w-6 [&_svg]:w-full [&_svg]:h-auto [&_svg]:stroke-uiBlack">
            {%- render 'icon-key-feature', icon: icon -%}
        </div>
    {%- endif -%}

    {%- if iconCustom != blank -%}
        <div class="w-6">
            <img
                class="w-full"
                srcset="{{ iconCustom | image_url: width: 24, height: 24 }} 1x,
                        {{ iconCustom | image_url: width: 48, height: 48 }} 2x"
                src="{{ iconCustom | image_url: width: 24, height: 24 }}"
                width="24"
                height="24"
                alt=""
                loading="lazy"
            >
        </div>
    {%- endif -%}
    <span class="body-s">{{ text }}</span>
</li>
